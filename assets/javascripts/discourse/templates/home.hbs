<div class="neo4j-home-grid">
  <div class="neo4j-home-grid-sidebar">
    <div class="neo4j-home-grid-announcements">
      {{mount-widget widget="latest-announcements"}}
    </div>
    <div class="neo4j-home-grid-blog">
      {{mount-widget widget="latest-blog-post"}}
    </div>
    <div class="neo4j-home-grid-tweets">
      {{mount-widget widget="latest-tweet"}}
    </div>
    <div class="neo4j-home-grid-neo4j-newbie">
      {{mount-widget widget="neo4j-newbie-links"}}
    </div>
    <div class="neo4j-home-grid-graph-gear-store">
      {{mount-widget widget="graph-gear-store-links"}}
    </div>
    <div class="neo4j-home-grid-jobs">
      {{mount-widget widget="jobs-links"}}
    </div>
    <div class="neo4j-home-grid-share-feedback">
      {{mount-widget widget="share-feedback-links"}}
    </div>
    <div class="neo4j-home-grid-links">
      {{mount-widget widget="useful-links"}}
    </div>
    <div class="neo4j-home-grid-sidebar-spacer">
    </div>
  </div>

  <div class="neo4j-home-grid-main">
    <div class="neo4j-home-grid-contents">
      {{mount-widget widget="custom-categories" categories_full=categories_full}}
    </div>
    <div class="neo4j-home-grid-help">
      {{mount-widget widget="can-you-help"}}
    </div>
    <div class="neo4j-home-grid-ninjas">
      {{mount-widget widget="ninjas"}}
    </div>
    <div class="neo4j-home-grid-featured-and-loves">
      <div class="neo4j-home-grid-featured">
        {{mount-widget widget="featured-member"}}
      </div>
      <div class="neo4j-home-grid-loves">
          {{mount-widget widget="neo4j-loves-developers"}}
      </div>
    </div>
    <div class="neo4j-home-grid-community-content">
      {{mount-widget widget="community-content"}}
    </div>
    <div class="neo4j-home-grid-community-projects">
      {{mount-widget widget="community-projects"}}
    </div>
    <div class="neo4j-home-grid-meet-new-members">
      {{mount-widget widget="meet-new-members"}}
    </div>
    <div class="neo4j-home-grid-certified-developers">
      {{mount-widget widget="certified-developers"}}
    </div>
    <div class="neo4j-home-grid-main-spacer">
    </div>
  </div>
  <div class="neo4j-home-grid-footer">
  </div>
</div>
